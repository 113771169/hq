	</parameter>
	<parameter name="SQL_BOOLEAN_TRUE" isForPrompting="false" class="java.lang.Boolean">
		<defaultValueExpression ><![CDATA[Boolean.TRUE]]></defaultValueExpression>
	</parameter>
	<parameter name="Now" isForPrompting="false" class="java.lang.Long">
		<defaultValueExpression ><![CDATA[new Long(System.currentTimeMillis())]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select 1 as type, p.id, p.name, pt.name as typename, p.ctime from EAM_PLATFORM p, EAM_PLATFORM_TYPE pt where p.platform_type_id = pt.id and (select count(m.id) from EAM_MEASUREMENT m, EAM_MEASUREMENT_TEMPL t, EAM_MONITORABLE_TYPE mt where m.template_id = t.id and t.monitorable_type_id = mt.id and mt.appdef_type=1 and m.instance_id=p.id and m.enabled=$P{SQL_BOOLEAN_TRUE} and m.coll_interval is not null) = 0
union
select 2 as type, s.id, s.name, st.name as typename, s.ctime from EAM_SERVER s, EAM_SERVER_TYPE st where s.server_type_id = st.id and (st.plugin != 'system' and st.plugin != 'netservices') and (select count(m.id) from EAM_MEASUREMENT m, EAM_MEASUREMENT_TEMPL t, EAM_MONITORABLE_TYPE mt where m.template_id = t.id and t.monitorable_type_id = mt.id and mt.appdef_type=2 and m.instance_id=s.id and m.enabled=$P{SQL_BOOLEAN_TRUE} and m.coll_interval is not null) = 0
union
select 3 as type, s.id, s.name, st.name as typename, s.ctime from EAM_SERVICE s, EAM_SERVICE_TYPE st where s.service_type_id = st.id and (select count(m.id) from EAM_MEASUREMENT m, EAM_MEASUREMENT_TEMPL t, EAM_MONITORABLE_TYPE mt where m.template_id = t.id and t.monitorable_type_id = mt.id and mt.appdef_type=3 and m.instance_id=s.id and m.enabled=$P{SQL_BOOLEAN_TRUE} and m.coll_interval is not null) = 0
order by ctime desc]]></queryString>

