		<defaultValueExpression ><![CDATA[new String("http://localhost:7080/")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT id, name, resourcetype, ctime, resourcetypeid FROM (
	    SELECT p.id AS id, p.name AS name, pt.name AS resourcetype, p.ctime AS ctime, 1 AS resourcetypeid
          FROM EAM_PLATFORM p, EAM_PLATFORM_TYPE pt
          WHERE p.platform_type_id = pt.id AND p.ctime > $P{startTime} AND p.ctime < $P{endTime}
        UNION ALL
        SELECT s.id AS id, s.name AS name, st.name AS resourcetype, s.ctime AS ctime, 2 AS resourcetypeid
          FROM EAM_SERVER s, EAM_SERVER_TYPE st
          WHERE s.server_type_id = st.id AND s.ctime > $P{startTime} AND s.ctime < $P{endTime}
        UNION ALL
        SELECT s.id AS id, s.name AS name, st.name AS resourcetype, s.ctime AS ctime, 3 AS resourcetypeid
          FROM EAM_SERVICE s, EAM_SERVICE_TYPE st
          WHERE s.service_type_id = st.id AND s.ctime > $P{startTime} AND s.ctime < $P{endTime}) tab ORDER BY ctime DESC]]></queryString>

