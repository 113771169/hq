	<subDataset name="ResourcesByTime" >

		<parameter name="startTime" isForPrompting="true" class="java.lang.Long"/>
		<parameter name="endTime" isForPrompting="true" class="java.lang.Long"/>
		<queryString><![CDATA[SELECT ctime, count(*) AS total FROM
              (SELECT p.ctime - (MOD(p.ctime, 86400000)) AS ctime FROM EAM_PLATFORM p, EAM_PLATFORM_TYPE pt
                 WHERE p.platform_type_id = pt.id AND p.ctime > $P{startTime} AND p.ctime < $P{endTime}
               UNION ALL
               SELECT s.ctime - (MOD(s.ctime, 86400000)) AS ctime FROM EAM_SERVER s, EAM_SERVER_TYPE st
                 WHERE s.server_type_id = st.id AND s.ctime > $P{startTime} AND s.ctime < $P{endTime}
               UNION ALL
               SELECT s.ctime - (MOD(s.ctime, 86400000)) AS ctime FROM EAM_SERVICE s, EAM_SERVICE_TYPE st
                 WHERE s.service_type_id = st.id AND s.ctime > $P{startTime} AND s.ctime < $P{endTime}) tab
            GROUP BY ctime]]></queryString>

		<field name="ctime" class="java.lang.Long"/>
		<field name="total" class="java.lang.Integer"/>

	</subDataset>

	<parameter name="StartDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[
            new java.util.Date(System.currentTimeMillis() - 86400000)
