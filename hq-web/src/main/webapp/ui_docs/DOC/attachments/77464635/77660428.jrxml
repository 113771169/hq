	<parameter name="endTime" isForPrompting="false" class="java.lang.Long">
		<defaultValueExpression ><![CDATA[
            new Long($P{EndDate}.getTime())
        ]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT COUNT(*) AS total, e.entity_type AS type, e.entity_id as id, r.name FROM EAM_EVENT_LOG e, EAM_RESOURCE r WHERE type='org.hyperic.hq.measurement.shared.ResourceLogEvent' AND e.timestamp > $P{startTime} AND e.timestamp <= $P{endTime} AND e.entity_id = r.instance_id AND ((entity_type = 1 AND r.resource_type_id = 301) OR (entity_type = 2 AND r.resource_type_id = 303) OR (entity_type = 3 AND r.resource_type_id = 305)) GROUP BY entity_type,entity_id, r.name ORDER BY total DESC]]></queryString>

