				<textFieldExpression   class="java.lang.String"><![CDATA[$F{name}]]></textFieldExpression>
						<hyperlinkReferenceExpression><![CDATA[new String($P{HYPERIC_BASE_URL} + "Resource.do?eid=" + $F{appdef_type} + ":" + $F{instance_id})]]></hyperlinkReferenceExpression>
