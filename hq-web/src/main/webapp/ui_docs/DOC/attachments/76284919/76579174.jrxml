					<imageExpression class="java.lang.String"><![CDATA["images/header.gif"]]></imageExpression>
						<hyperlinkReferenceExpression><![CDATA[new String($P{HYPERIC_BASE_URL} + "reporting/ReportCenter.do")]]></hyperlinkReferenceExpression>
				</image>
				<image  evaluationTime="Now" hyperlinkType="Reference"  hyperlinkTarget="Self" >
					<reportElement
