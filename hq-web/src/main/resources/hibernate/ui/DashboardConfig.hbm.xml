<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.ui.server.session">
  <class name="DashboardConfig" table="EAM_DASH_CONFIG">
    <cache usage="read-write"/>

    <id name="id" type="integer">
      <meta attribute="scope-set"><![CDATA[private]]></meta>
      <column name="ID" not-null="true"/>
      <generator class="org.hyperic.hibernate.id.ComboGenerator">
        <param name="sequence">EAM_DASH_CONFIG_ID_SEQ</param>
      </generator>
    </id>

    <discriminator column="CONFIG_TYPE" type="string"/>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <property name="name">
      <column name="name" length="255" not-null="true"/>
    </property>

    <many-to-one name="crispo">
      <column name="CRISPO_ID" not-null="true" index="DASH_CONFIG_CRISPO_ID_IDX"/>
    </many-to-one>

    <subclass name="RoleDashboardConfig" discriminator-value="ROLE">
      <many-to-one name="role">
        <column name="ROLE_ID" unique-key="role_dash"/>
      </many-to-one>
    </subclass>
    
    <subclass name="UserDashboardConfig" discriminator-value="USER">
      <many-to-one name="user">
        <column name="USER_ID" unique-key="user_dash"/>
      </many-to-one>
    </subclass>

  </class>

</hibernate-mapping>
