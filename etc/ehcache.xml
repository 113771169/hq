         maxElementsInMemory="5000"
         eternal="true"
         timeToIdleSeconds="0"
         timeToLiveSeconds="0"
         memoryStoreEvictionPolicy="LRU"/>
  <cache name="org.hyperic.hq.measurement.server.session.MonitorableType"
         maxElementsInMemory="1000"
