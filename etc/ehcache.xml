         maxElementsInMemory="20000"
         eternal="true"
         timeToIdleSeconds="0"
         timeToLiveSeconds="0"
         memoryStoreEvictionPolicy="LRU"/>
  <cache name="org.hyperic.hq.measurement.server.session.MeasurementTemplate"
         maxElementsInMemory="20000"
         eternal="true"
         timeToIdleSeconds="0"
         timeToLiveSeconds="0"
         memoryStoreEvictionPolicy="LRU"/>
