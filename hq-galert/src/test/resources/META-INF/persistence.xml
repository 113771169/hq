<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="spring-jpa" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>org.hyperic.hq.appdef.server.session.ResourceAuxLogPojo</class>
		<class>org.hyperic.hq.galerts.server.session.ExecutionStrategyTypeInfo</class>
		<class>org.hyperic.hq.galerts.server.session.ExecutionStrategyInfo</class>
		<class>org.hyperic.hq.galerts.server.session.GalertDef</class>
		<class>org.hyperic.hq.galerts.server.session.GalertLog</class>
		<class>org.hyperic.hq.galerts.server.session.GalertActionLog</class>
		<class>org.hyperic.hq.galerts.server.session.GalertAuxLog</class>
		<class>org.hyperic.hq.galerts.server.session.GtriggerInfo</class>
		<class>org.hyperic.hq.galerts.server.session.GtriggerTypeInfo</class>
		<class>org.hyperic.hq.escalation.server.session.Escalation</class>
		<class>org.hyperic.hq.escalation.server.session.EscalationState</class>
		<class>org.hyperic.hq.events.server.session.Action</class>
		<class>org.hyperic.hq.events.server.session.ResourceAlertDefinition</class>
		<class>org.hyperic.hq.events.server.session.ResourceTypeAlertDefinition</class>
		<class>org.hyperic.hq.events.server.session.AlertDefinitionState</class>
		<class>org.hyperic.hq.events.server.session.AlertActionLog</class>
		<class>org.hyperic.hq.events.server.session.AlertConditionLog</class>
		<class>org.hyperic.hq.events.server.session.Alert</class>
		<class>org.hyperic.hq.events.server.session.AlertCondition</class>
		<class>org.hyperic.hq.events.server.session.RegisteredTrigger</class>
		<class>org.hyperic.hq.measurement.server.session.MonitorableType</class>
		<class>org.hyperic.hq.measurement.server.session.Category</class>
		<class>org.hyperic.hq.measurement.server.session.MeasurementTemplate</class>
		<class>org.hyperic.hq.measurement.server.session.Measurement</class>
		<class>org.hyperic.hq.measurement.server.session.AvailabilityDataRLE</class>
		<class>org.hyperic.hq.measurement.server.session.Baseline</class>
		<class>org.hyperic.hq.measurement.server.session.MetricAuxLogPojo</class>
		<class>org.hyperic.hq.auth.domain.AuthzSubject</class>
		<class>org.hyperic.hq.auth.domain.Role</class>
		<class>org.hyperic.hq.auth.domain.RoleCalendar</class>
		<class>org.hyperic.hq.auth.domain.Calendar</class>
		<class>org.hyperic.hq.auth.domain.CalendarEntry</class>
		<class>org.hyperic.hq.config.domain.Crispo</class>
		<class>org.hyperic.hq.config.domain.CrispoOption</class>
		<shared-cache-mode>ENABLE_SELECTIVE</shared-cache-mode>
		<properties>
			<property name="hibernate.hbm2ddl.auto" value="update" />
			<property name="hibernate.dialect" value="org.hyperic.hq.test.HSQLDialect" />
			<property name="hibernate.cache.provider_class"
				value="net.sf.ehcache.hibernate.SingletonEhCacheProvider" />
		</properties>
	</persistence-unit>
</persistence>
