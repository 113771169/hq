<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.rt">

  <class name="RtErrorCode" table="EAM_ERROR_CODE">
    <id name="id" type="integer">
      <column name="ID" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">EAM_ERROR_CODE_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <property name="code" type="integer" unique="true">
      <column name="CODE" index="ERROR_CODE" not-null="true"/>
    </property>

    <property name="desc" type="string">
      <column name="DESCRIPTION" not-null="true" length="64"/>
    </property>

    <set inverse="true" cascade="save-update,delete,evict,persist,merge" name="rtStatErrors" table="EAM_STAT_ERRORS">
      <key on-delete="cascade" column="ERROR_ID"/>
      <one-to-many class="RtStatError"/>
    </set>

  </class>

</hibernate-mapping>