<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.events.server.session">

  <class name="Event" table="EAM_EVENT">
    <id name="id" type="long">
      <meta attribute="scope-set"><![CDATA[private]]></meta>
      <column name="ID" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">EAM_EVENT_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <property lazy="true" name="eventObject" type="binary">
      <column name="EVENT_OBJECT" not-null="true"/>
    </property>

    <property name="ctime" type="long">
      <column name="CTIME" not-null="true" index="EVENT_CTIME_IDX"/>
    </property>

    <bag inverse="true" cascade="save-update,delete,evict,persist,merge" name="triggerEventsBag" table="EAM_TRIGGER_EVENT">
      <key on-delete="cascade" column="EVENT_ID"/>
      <one-to-many class="TriggerEvent"/>
    </bag>
  </class>

</hibernate-mapping>
