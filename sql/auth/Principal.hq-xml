<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
  package="org.hyperic.hq.auth">

  <class name="Principal" table="EAM_PRINCIPAL">
    <id name="id" type="integer">
      <meta attribute="scope-set">private</meta>
      <column name="ID" not-null="true" />
      <generator class="sequence">
        <param name="sequence">EAM_PRINCIPAL_ID_SEQ</param>
      </generator>
    </id>

    <natural-id mutable="true">
      <property name="principal" type="string">
        <column name="PRINCIPAL" not-null="true" length="64" />
      </property>
    </natural-id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0" />
    </version>

    <property name="password" type="string">
      <column name="PASSWORD" not-null="true" length="64"/>
    </property>

  </class>
</hibernate-mapping>
