<?xml version="1.0" encoding="UTF-8"?>
<Covalent.DBSetup name="appdef-schema.xml" notice="DBSetup, Copyright (C) 2002, Covalent Technologies, Inc., All Rights Reserved.">

    <table name="EAM_CONFIG_PROPS">
        <column name="ID" default="sequence-only" initial="10001"
            primarykey="true" required="true" type="INTEGER"/>
        <column name="PREFIX" type="VARCHAR2" size="80" required="false"/>
        <column name="PROPKEY" type="VARCHAR2" size="80"/>
        <column name="PROPVALUE" type="VARCHAR2" size="300"/>
        <column name="DEFAULT_PROPVALUE" type="VARCHAR2" size="300" />
        <column name="FREAD_ONLY" type="BOOLEAN" default="FALSE" />

        <index name="EAM_CONFIGURATION_KEY_INDX" unique="true">
            <field ref="PREFIX"/>
            <field ref="PROPKEY"/>
        </index>
    </table>

</Covalent.DBSetup>
