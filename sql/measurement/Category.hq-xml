<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hyperic.hq.measurement.server.session">

  <class name="Category" table="EAM_MEASUREMENT_CAT">
    <cache usage="read-only"/>

    <id name="id" type="integer">
      <column name="ID" not-null="true" />
      <generator class="sequence">
        <param name="sequence">EAM_MEASUREMENT_CAT_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0" />
    </version>

    <property name="name" type="string">
      <column name="NAME" not-null="true" length="100" unique="true" />
    </property>

    <property name="cid" type="integer">
      <column name="CID" />
    </property>

  </class>

</hibernate-mapping>
