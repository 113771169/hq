<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hyperic.hq.measurement.server.session">
  <class name="MetricAuxLogPojo" table="EAM_METRIC_AUX_LOGS">

    <cache usage="read-write" />
    <id name="id" type="integer">
      <column name="ID" not-null="true" />
      <generator class="sequence">
        <param name="sequence">EAM_METRIC_AUX_LOG_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0" />
    </version>

    <many-to-one name="auxLog">
      <column name="AUX_LOG_ID" index="AUX_LOG_ID_IDX" not-null="true"/>
    </many-to-one>
    
    <many-to-one name="metric">
      <column name="METRIC_ID" not-null="true"/>
    </many-to-one>
  </class>
</hibernate-mapping>
