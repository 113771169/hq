<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.events.server.session">

  <class name="FiredTrigger" table="EAM_FIRED_TRIGGER">
    <id name="id" type="integer">
      <meta attribute="scope-set"><![CDATA[private]]></meta>
      <column name="ID" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">EAM_FIRED_TRIGGER_ID_SEQ</param>
      </generator>
    </id>

    <property name="triggerId" type="integer">
      <column name="TRIGGER_ID" not-null="true" index="EAM_FIRED_TRIGGER_IDX"/>
    </property>

    <property name="timestamp" type="long">
      <column name="TIMESTAMP" not-null="true"/>
    </property>
  </class>

</hibernate-mapping>