<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.galerts.server.session">

  <class name="GalertActionLog" table="EAM_GALERT_ACTION_LOG">
    <cache usage="read-write"/>
    <id name="id" type="integer">
      <column name="ID" not-null="true"/>
      <generator class="org.hyperic.hibernate.id.ComboGenerator">
        <param name="sequence">EAM_GALERT_ACTION_LOG_ID_SEQ</param>
      </generator>
    </id>

    <property name="timeStamp">
      <column name="TIMESTAMP" not-null="true"/>
    </property>

    <property name="detail">
      <column name="DETAIL" not-null="true" length="500"/>
    </property>

    <many-to-one name="galertLog">
      <column name="GALERT_ID" not-null="true" index="GALERT_ACTION_LOG_IDX"/>
    </many-to-one>

	<property name="alertTypeEnum">
	  <column name="ALERT_TYPE" not-null="true"/>
	</property>

    <many-to-one name="action">
      <column name="ACTION_ID"/>
    </many-to-one>

    <many-to-one name="subject">
      <column name="SUBJECT_ID"/>
    </many-to-one>
    

  </class>

</hibernate-mapping>
