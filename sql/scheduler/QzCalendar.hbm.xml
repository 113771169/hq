<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.scheduler">

  <class name="QzCalendar" table="QRTZ_CALENDARS">
    <id name="calendarName" type="string">
      <meta attribute="use-in-equals"><![CDATA[true]]></meta>
      <column name="CALENDAR_NAME" length="80" not-null="true"/>
      <generator class="assigned"/>
    </id>

    <property lazy="true" name="calendar" type="binary">
      <column name="CALENDAR" not-null="true"/>
    </property>
  </class>

</hibernate-mapping>
