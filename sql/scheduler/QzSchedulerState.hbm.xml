<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.scheduler">

  <class name="QzSchedulerState" table="QRTZ_SCHEDULER_STATE">
    <id name="instanceName" type="string">
      <meta attribute="use-in-equals"><![CDATA[true]]></meta>
      <column name="INSTANCE_NAME" length="80" not-null="true"/>
      <generator class="assigned"/>
    </id>

    <property name="lastCheckinTime" type="long">
      <column name="LAST_CHECKIN_TIME" not-null="true"/>
    </property>

    <property name="checkinInterval" type="long">
      <column name="CHECKIN_INTERVAL" not-null="true"/>
    </property>

    <property name="recoverer" type="string">
      <column name="RECOVERER" not-null="false" length="80"/>
    </property>
  </class>

</hibernate-mapping>
