<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.hyperic.hq.common.shared">
  <class name="SequenceNumber" table="HQ_SEQUENCE">
    <composite-id name="id" class="SequenceNumberId">
      <key-property name="seqName" type="string">
        <column name="seq_name" not-null="true" length="50"/>
      </key-property>

      <key-property name="seqValue" type="integer">
        <column name="seq_val" not-null="true"/>
      </key-property>
    </composite-id>
  </class>
</hibernate-mapping>
