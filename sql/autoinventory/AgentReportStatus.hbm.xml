<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.autoinventory.server.session">

  <class name="AgentReportStatus" table="EAM_AI_AGENT_REPORT">
    <id name="id" type="integer">
      <meta attribute="scope-set"><![CDATA[private]]></meta>
      <column name="ID" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">EAM_AI_AGENT_REPORT_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <many-to-one name="agent">
      <column name="AGENT_ID" unique="true" not-null="true"/>
    </many-to-one>

    <property name="reportTime">
      <column name="REPORT_TIME" not-null="true"/>
    </property>
    
    <property name="serviceDirty">
      <column name="SERVICE_DIRTY" not-null="true"/>
    </property>
  </class>

</hibernate-mapping>
