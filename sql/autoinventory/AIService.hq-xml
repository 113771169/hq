<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
  package="org.hyperic.hq.autoinventory">

  <class name="AIService" table="EAM_AIQ_SERVICE">
    <id name="id" type="integer">
      <meta attribute="scope-set">private</meta>
      <column name="ID" not-null="true" />
      <generator class="sequence">
        <param name="sequence">EAM_AIQ_SERVICE_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name ="VERSION_COL" default="0" />
    </version>

    <property name="name" type="string">
      <column name="NAME" index="AIQ_SERVICE_NAME" not-null="true" length="200" />
    </property>

    <property name="description" type="string">
      <column name="DESCRIPTION" not-null="false" length="300" />
    </property>

    <property name="serviceTypeName" type="string">
      <column name="SERVICETYPENAME" not-null="true" length="200" />
    </property>

    <property name="queueStatus" type="integer">
      <column name="QUEUESTATUS" not-null="false" />
    </property>

    <property name="diff" type="long">
      <column name="DIFF" />
    </property>

    <property name="ignored" type="boolean">
      <column name="IGNORED" />
    </property>

    <property name="creationTime" type="long">
      <column name="CTIME" />
    </property>

    <property name="modifiedTime" type="long">
      <column name="MTIME" />
    </property>

    <property name="customProperties" type="binary">
      <column name="CUSTOM_PROPERTIES" />
    </property>

    <property name="productConfig" type="binary">
      <column name="PRODUCT_CONFIG" not-null="false" />
    </property>

    <property name="controlConfig" type="binary">
      <column name="CONTROL_CONFIG" not-null="false" />
    </property>

    <property name="measurementConfig" type="binary">
      <column name="MEASUREMENT_CONFIG" not-null="false" />
    </property>

    <property name="responseTimeConfig" type="binary">
      <column name="RESPONSETIME_CONFIG" not-null="false" />
    </property>

    <many-to-one name="server" class="org.hyperic.hq.appdef.Server">
      <column name="SERVER_ID" not-null="false" />
    </many-to-one>
  </class>

</hibernate-mapping>
