<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hyperic.hq.appdef">

  <class name="AgentType" table="EAM_AGENT_TYPE">
    <cache usage="read-write"/>

    <id name="id" type="integer">
      <meta attribute="scope-set"><![CDATA[private]]></meta>
      <column name="ID" not-null="true"/>
      <generator class="sequence">
        <param name="sequence">EAM_AGENT_TYPE_ID_SEQ</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name="VERSION_COL" default="0"/>
    </version>

    <property name="name" type="string">
      <column name="NAME" length="80"/>
    </property>

    <property name="sortName" type="string">
      <column name="SORT_NAME" length="80"/>
    </property>

    <property name="creationTime" type="long">
      <column name="CTIME"/>
    </property>

    <property name="modifiedTime" type="long">
      <column name="MTIME"/>
    </property>

    <bag inverse="true" cascade="save-update,delete,evict,persist,merge" name="agents" table="EAM_AGENT" lazy="true">
      <key on-delete="cascade" column="AGENT_TYPE_ID"/>
      <one-to-many class="Agent"/>
    </bag>
  </class>

</hibernate-mapping>