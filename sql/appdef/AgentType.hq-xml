<?xml version="1.0" encoding="iso-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
  package="org.hyperic.hq.appdef.shared.pojo">

  <class name="AgentType" table="EAM_AGENT_TYPE">
    <id name="id" type="integer">
      <meta attribute="scope-set">private</meta>
      <column name="ID" not-null="true" />
      <generator class="sequence">
        <param name="sequence">EAM_AGENT_TYPE_ID_SEQ</param>
        <!-- postgres flavor -->
        <param name="parameters" dialect="PostgreSQL">start 10001 increment 1 cache 10</param>
        <!-- oracle flavor -->
        <param name="parameters" dialect="Oracle, Oracle9">start with 10001 increment by 1 cache 10</param>
      </generator>
    </id>

    <version name="_version_" type="long">
      <column name ="VERSION_COL" default="0" />
    </version>

    <property name="name" type="string">
      <column name="NAME" length="80" />
    </property>

    <property name="sortName" type="string">
      <column name="SORT_NAME" length="80" />
    </property>

    <property name="ctime" type="long">
      <column name="CTIME" />
    </property>

    <property name="mtime" type="long">
      <column name="MTIME" />
    </property>

    <bag name="agents" table="EAM_AGENT"
         cascade="save-update,delete,evict,delete-orphan"
         inverse="true" lazy="true">
      <key column="AGENT_TYPE_ID" />
      <one-to-many class="Agent" />
    </bag>
  </class>

</hibernate-mapping>
