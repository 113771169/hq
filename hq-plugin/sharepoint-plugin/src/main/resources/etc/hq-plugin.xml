            <metric name="Availability" template="SharePoint Foundation BDC Metadata:Type=Availability,Platform=Win32:Cache misses per second" category="AVAILABILITY"/>
            <metric name="Cache misses per second" indicator="true"/>
            <metric name="Cache hits per second" indicator="true" category="UTILIZATION"/>
        </service>



        <service name="Foundation Search Gatherer Databases">
            <filter name="template" value="SharePoint Foundation Search Gatherer Databases(_total):Platform=Win32:${name}"/>
            <metric name="Availability" template="SharePoint Foundation Search Gatherer Databases(_total):Type=Availability,Platform=Win32:Documents in the crawl history" category="AVAILABILITY"/>
            <metric name="Documents in the crawl history" indicator="true"/>
            <metric name="Documents in the crawl queue" indicator="true"/>
            <metric name="Links Waiting to be processed" indicator="true"/>
            <metric name="Links processed" indicator="true"/>
        </service>
    </server>

    <server name="SharePoint" version="2007" include="SharePoint 2010">
        <property name="version" value="SOFTWARE\Microsoft\Shared Tools\Web Server Extensions\12.0"/>
    </server>
</plugin>
