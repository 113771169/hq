<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.hyperic.hq.plugin</groupId>
        <artifactId>hq-plugin-parent</artifactId>
        <version>4.6.0.BUILD-SNAPSHOT</version>
    </parent>

    <artifactId>rabbitmq-plugin</artifactId>
    <packaging>jar</packaging>
    <name>Hyperic HQ Plugin - RabbitMQ Plugin</name>

    <dependencies>
        <dependency>
            <groupId>org.hyperic.hq.agent</groupId>
            <artifactId>hq-agent-core</artifactId>
            <version>${project.version}</version>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.hyperic.hq.hqapi</groupId>
            <artifactId>hqapi1</artifactId>
            <scope>compile</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

    <repositories>
        <repository>
            <id>hqapi-snapshot</id>
            <name>HQApi Snapshot Repository</name>
            <url>http://maven.hyperic.org/hqapi/snapshot</url>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>hqapi-release</id>
            <name>HQApi Release Repository</name>
            <url>http://maven.hyperic.org/hqapi/release</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>hqapi-milestone</id>
            <name>HQApi Milestone Repository</name>
            <url>http://maven.hyperic.org/hqapi/milestone</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
    </repositories>

</project>
