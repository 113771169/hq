<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jboss PUBLIC "-//JBoss//DTD JBOSS 3.0//EN" "http://www.jboss.org/j2ee/dtd/jboss_3_0.dtd">

<jboss>

	<enterprise-beans>

		<!--
			To add beans that you have deployment descriptor info for, add a file
			to your XDoclet merge directory called jboss-beans.xml that contains
			the <session></session>, <entity></entity> and
			<message-driven></message-driven> markup for those beans.
		-->

		
		<session>
			<ejb-name>ConfigManager</ejb-name>
			<local-jndi-name>LocalConfigManager</local-jndi-name>

		</session>
		

	
		<session>
			<ejb-name>GalertBoss</ejb-name>
			<jndi-name>ejb/bizapp/GalertBoss</jndi-name>
			<local-jndi-name>LocalGalertBoss</local-jndi-name>

		</session>
		<session>
			<ejb-name>DashboardPortletBoss</ejb-name>
			<jndi-name>ejb/bizapp/DashboardPortletBoss</jndi-name>
			<local-jndi-name>LocalDashboardPortletBoss</local-jndi-name>

		</session>
		<session>
			<ejb-name>EscalationManager</ejb-name>
			<local-jndi-name>LocalEscalationManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>AIBoss</ejb-name>
			<jndi-name>ejb/bizapp/AIBoss</jndi-name>
			<local-jndi-name>LocalAIBoss</local-jndi-name>

		</session>
		
		
		<session>
			<ejb-name>AppdefStatManager</ejb-name>
			<local-jndi-name>LocalAppdefStatManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>ServiceManager</ejb-name>
			<local-jndi-name>LocalServiceManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>LatherBoss</ejb-name>
			<jndi-name>ejb/bizapp/LatherBoss</jndi-name>
			<local-jndi-name>LocalLatherBoss</local-jndi-name>

		</session>
		<session>
			<ejb-name>ApplicationManager</ejb-name>
			<local-jndi-name>LocalApplicationManager</local-jndi-name>

		</session>
		
		<session>
			<ejb-name>ProductBoss</ejb-name>
			<jndi-name>ejb/bizapp/ProductBoss</jndi-name>
			<local-jndi-name>LocalProductBoss</local-jndi-name>

		</session>
		<session>
			<ejb-name>ControlBoss</ejb-name>
			<jndi-name>ejb/bizapp/ControlBoss</jndi-name>
			<local-jndi-name>LocalControlBoss</local-jndi-name>

		</session>
		<session>
			<ejb-name>DataManager</ejb-name>
			<local-jndi-name>LocalDataManager</local-jndi-name>

		</session>
		
		<session>
			<ejb-name>LiveDataBoss</ejb-name>
			<local-jndi-name>LocalLiveDataBoss</local-jndi-name>

		</session>
		<session>
			<ejb-name>AppdefManager</ejb-name>
			<local-jndi-name>LocalAppdefManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>PlatformManager</ejb-name>
			<local-jndi-name>LocalPlatformManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>ServerConfigManager</ejb-name>
			<local-jndi-name>LocalServerConfigManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>ServerManager</ejb-name>
			<local-jndi-name>LocalServerManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>AIQueueManager</ejb-name>
			<local-jndi-name>LocalAIQueueManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>UIPluginManager</ejb-name>
			<local-jndi-name>LocalUIPluginManager</local-jndi-name>

		</session>
		<session>
			<ejb-name>MeasurementBoss</ejb-name>
			<jndi-name>ejb/bizapp/MeasurementBoss</jndi-name>
			<local-jndi-name>LocalMeasurementBoss</local-jndi-name>

		</session>
		
		<!--
			write a merge file jboss-webservices.ent for webservice-description
		-->

	</enterprise-beans>

	<resource-managers>
	</resource-managers>

	<!--
		| for container settings, you can merge in jboss-container.xml | this
		can contain <invoker-proxy-bindings/> and <container-configurations/>
	-->

</jboss>
